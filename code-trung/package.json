{
  "name": "fresher-client",
  "version": "0.0.1",
  "description": "fresherClient",
  "main": "index.html",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.dev.js",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "server": "http-server -g",
    "test": "mocha  --require  babel-register",
    "tests": "mocha  --require babel-register --recursive './src/js/**/__tests__/*.*'",
    "eslint": "eslint './src/js/**/*.*'",
    "stylelint": "stylelint './src/css/*.*'",
    "css": "node-sass ./src/css/style.scss ./public/css/style.css",
    "watch-css": "node-sass -w ./src/css/style.scss ./public/css/style.css",
    "webpack": "webpack --config=webpack.dev.js",
    "build:dev": "node-sass ./src/css/style.scss ./public/css/style.css && webpack --config=webpack.dev.js",
    "build:test": "node-sass ./src/css/style.scss ./public/css/style.css --output-style compressed && webpack --config=webpack.test.js",
    "build:staging": "node-sass ./src/css/style.scss ./public/css/style.css --output-style compressed && webpack --config=webpack.staging.js",
    "build:production": "node-sass ./src/css/style.scss ./public/css/style.css --output-style compressed && webpack --config=webpack.prod.js"
  },
  "author": "@fresher",
  "private": true,
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "reactstrap": "^8.1.1",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.1",
    "styled-components": "^4.4.1",
    "validator": "^12.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-notes": "^5.2.5",
    "@storybook/addon-viewport": "^5.2.5",
    "@storybook/addons": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "jsdom": "^15.2.1",
    "mocha": "^6.2.2",
    "node-sass": "^4.13.0",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^19.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  }
}
